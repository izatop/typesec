{
  "name": "typesec",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "check": "bun tsc -b packages",
    "check:watch": "bun tsc -b packages -w",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "oxlint",
    "up": "bun update --latest && bun run -F '*' upgrade && bun i"
  },
  "devDependencies": {
    "@prettier/plugin-oxc": "^0.0.4",
    "@types/bun": "^1.2.19",
    "oxlint": "^1.10.0",
    "prettier": "3.6.2",
    "typescript": "^5.9.2"
  },
  "private": true
}
