{
  "name": "typesec",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "check": "bun tsc -b packages",
    "check:watch": "bun tsc -b packages -w",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "up": "bun update --latest && bun run -F '*' upgrade"
  },
  "devDependencies": {
    "prettier": "3.6.2",
    "typescript": "^5.8.3",
    "@types/bun": "^1.2.19"
  },
  "private": true
}
